{
  "name": "@msq-tx-monitor/tx-dashboard",
  "version": "1.0.0",
  "description": "React dashboard for MSQ transaction monitoring with real-time updates",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "echo 'Dashboard requires NX build - use: pnpm build:nx'",
    "build:nx": "nx build tx-dashboard",
    "preview": "vite preview",
    "test": "vitest",
    "test:nx": "nx test tx-dashboard",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:nx": "nx lint tx-dashboard",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix",
    "format": "prettier --check \"src/**/*.{ts,tsx}\"",
    "format:fix": "prettier --write \"src/**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit --project tsconfig.app.json"
  },
  "dependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.0.0",
    "recharts": "^2.8.0",
    "socket.io-client": "^4.7.0",
    "web3": "^4.8.0",
    "@tanstack/react-query": "^5.0.0",
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.0.0",
    "vite": "^5.2.11",
    "vitest": "^1.6.0",
    "@testing-library/react": "^14.0.0",
    "@testing-library/jest-dom": "^6.0.0",
    "jsdom": "^24.0.0"
  },
  "keywords": [
    "react",
    "dashboard",
    "real-time",
    "blockchain",
    "transaction",
    "monitoring",
    "msq"
  ]
}
