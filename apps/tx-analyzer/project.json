{
  "name": "tx-analyzer",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/tx-analyzer/src",
  "tags": ["type:app", "scope:backend", "lang:typescript"],
  "targets": {
    "serve": {
      "executor": "nx:run-commands",
      "defaultConfiguration": "development",
      "options": {
        "cwd": "apps/tx-analyzer",
        "command": "tsx watch --clear-screen=false src/main.ts"
      },
      "configurations": {
        "development": {
          "command": "tsx watch --clear-screen=false src/main.ts"
        },
        "production": {
          "command": "node dist/main.js"
        }
      }
    },
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "echo 'Skipping tx-analyzer build (excluded from monorepo)'"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "echo 'Skipping tx-analyzer test (excluded from monorepo)'"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "command": "echo 'Skipping tx-analyzer lint (excluded from monorepo)'"
      }
    },
    "typecheck": {
      "executor": "nx:run-commands",
      "options": {
        "command": "echo 'Skipping tx-analyzer typecheck (excluded from monorepo)'"
      }
    }
  }
}
