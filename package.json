{
  "name": "msq-tx-monitor",
  "version": "1.0.0",
  "description": "Real-time Blockchain Transaction Monitoring System for MSQ Wallet Tokens",
  "keywords": [
    "blockchain",
    "transaction-monitoring",
    "polygon",
    "msq",
    "real-time",
    "web3",
    "nx-monorepo"
  ],
  "author": "",
  "license": "MIT",
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "scripts": {
    "dev": "nx run-many --target=serve --projects=tx-dashboard,tx-api,chain-scanner,tx-analyzer --parallel",
    "build": "nx run-many --target=build --all",
    "test": "nx run-many --target=test --all",
    "lint": "nx run-many --target=lint --all",
    "lint:fix": "nx run-many --target=lint --all --fix",
    "format": "nx format:check",
    "format:fix": "nx format:write",
    "typecheck": "nx run-many --target=typecheck --all",
    "docker:dev": "docker-compose -f docker/docker-compose.dev.yml up -d",
    "docker:prod": "docker-compose -f docker/docker-compose.yml up -d",
    "docker:down": "docker-compose -f docker/docker-compose.dev.yml down && docker-compose -f docker/docker-compose.yml down",
    "clean": "nx reset && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "nx": "21.5.2",
    "@nx/eslint": "21.5.2",
    "@nx/eslint-plugin": "21.5.2",
    "@nx/js": "21.5.2",
    "@nx/node": "21.5.2",
    "@nx/react": "21.5.2",
    "@nx/vite": "21.5.2",
    "@nx/webpack": "21.5.2",
    "@nx/jest": "21.5.2",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "@typescript-eslint/eslint-plugin": "7.10.0",
    "@typescript-eslint/parser": "7.10.0",
    "prettier": "3.2.5",
    "typescript": "5.4.5",
    "jest": "29.7.0",
    "@types/jest": "29.5.12",
    "@types/node": "20.12.8",
    "ts-jest": "29.1.4",
    "ts-node": "10.9.2",
    "vite": "5.2.11",
    "vitest": "1.6.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "@types/react": "18.3.1",
    "@types/react-dom": "18.3.0",
    "express": "4.19.2",
    "@types/express": "4.17.21",
    "web3": "4.8.0",
    "@types/web3": "1.2.2",
    "mysql2": "3.9.7",
    "redis": "4.6.14",
    "@types/redis": "4.0.10"
  }
}
